{
  "name": "markdown-translate",
  "version": "2.0.1",
  "description": "Translate Markdown files",
  "main": "src/index.js",
  "files": [
    "defaults.js",
    "src/**/*.js"
  ],
  "bin": "./bin.js",
  "type": "module",
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=18.13.0"
  },
  "scripts": {
    "postinstall": "chmod +x bin.js",
    "test": "jest",
    "lint": "cross-env ./node_modules/.bin/eslint .",
    "transpile": "cross-env ./node_modules/.bin/rollup --config ./rollup.config.js",
    "compile": "cross-env ./node_modules/.bin/pkg output/bin.js -t latest-linux-x64,latest-win-x64,latest-macos-x64 --out-path ./dist -C GZip",
    "build": "npm run clean && npm run transpile && npm run compile",
    "pack:dry": "npm pack --dry-run",
    "publish:dry": "npm publish --dry-run",
    "clean": "./node_modules/.bin/rimraf dist output"
  },
  "keywords": [
    "markdown",
    "translate",
    "deepl",
    "ibm",
    "js",
    "cli"
  ],
  "license": "ISC",
  "dependencies": {
    "chalk": "^5.3.0",
    "deepl-node": "^1.9.0",
    "gray-matter": "^4.0.3",
    "ibm-watson": "^8.0.0",
    "node-fetch": "^3.3.1",
    "rehype-document": "^6.1.0",
    "rehype-format": "^4.0.1",
    "rehype-parse": "^8.0.4",
    "rehype-raw": "^7.0.0",
    "rehype-remark": "^9.1.2",
    "rehype-stringify": "^9.0.3",
    "remark-gfm": "^3.0.1",
    "remark-html": "^15.0.2",
    "remark-mdx": "^2.3.0",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^10.1.0",
    "remark-stringify": "^10.0.3",
    "unified": "^10.1.2",
    "unist-util-visit": "^5.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@babel/preset-env": "^7.22.7",
    "@rollup/plugin-commonjs": "^25.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^5.0.5",
    "babel-jest": "^29.6.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.41.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.6.1",
    "jest-fetch-mock": "^3.0.3",
    "mdast-util-to-markdown": "^2.1.0",
    "npm-cli-login": "^1.0.0",
    "pkg": "^5.8.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.5",
    "rollup": "^3.26.2",
    "rollup-jest": "^3.1.0",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "unist-builder": "^4.0.0"
  },
  "author": {
    "name": "Jake Bukuts",
    "email": "jbukuts@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbukuts/markdown-translate.git"
  },
  "bugs": {
    "url": "https://github.com/jbukuts/markdown-translate/issues"
  },
  "homepage": "https://github.com/jbukuts/markdown-translate"
}
