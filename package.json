{
  "name": "markdown-translate",
  "version": "1.0.0",
  "description": "Translate Markdown files",
  "main": "index.js",
  "bin": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "transpile": "rollup index.js --config ./rollup.config.js",
    "build": "pkg output/index.js -t latest-linux-x64,latest-win-x64,latest-macos-x64,latest-macos-arm64 --out-path ./dist",
    "compile": "npm run transpile && npm run build"
  },
  "keywords": [
    "translate",
    "deepl",
    "js"
  ],
  "author": "Jake Bukuts",
  "license": "ISC",
  "dependencies": {
    "deepl-node": "^1.9.0",
    "form-data": "^4.0.0",
    "gray-matter": "^4.0.3",
    "ibm-watson": "^8.0.0",
    "node-fetch": "^3.3.1",
    "rehype-document": "^6.1.0",
    "rehype-format": "^4.0.1",
    "rehype-parse": "^8.0.4",
    "rehype-remark": "^9.1.2",
    "rehype-stringify": "^9.0.3",
    "remark-gfm": "^3.0.1",
    "remark-html": "^15.0.2",
    "remark-mdx": "^2.3.0",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^10.1.0",
    "remark-stringify": "^10.0.3",
    "unified": "^10.1.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "eslint": "^8.41.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "rollup-plugin-polyfill-node": "^0.12.0"
  }
}
